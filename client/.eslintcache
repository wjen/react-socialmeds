s",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/pages/Home.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/pages/Login.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/MenuBar.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/PostCard.js",["84"],"import React, { useContext } from 'react';\nimport { Button, Card, Icon, Label, Image, Popup } from 'semantic-ui-react';\nimport { Link } from 'react-router-dom';\nimport moment from 'moment';\nimport { AuthContext } from '../context/auth';\nimport LikeButton from './LikeButton';\nimport DeleteButton from './DeleteButton';\nimport EditModal from './EditModal';\nimport { MyPopup } from '../util/MyPopup';\n\nfunction PostCard({\n  post: { body, createdAt, id, username, likeCount, commentCount, likes },\n}) {\n  const { user } = useContext(AuthContext);\n\n  return (\n    // fluid takes up container width\n    <Card fluid>\n      <Card.Content>\n        <Image\n          floated=\"right\"\n          size=\"mini\"\n          src=\"https://react.semantic-ui.com/images/avatar/large/molly.png\"\n        />\n        <Card.Header>{username}</Card.Header>\n        <Card.Meta as={Link} to={`/posts/${id}`}>\n          {moment(createdAt).fromNow(true)}\n        </Card.Meta>\n        <Card.Description>{body}</Card.Description>\n      </Card.Content>\n      <Card.Content extra>\n        <LikeButton user={user} post={{ id, likes, likeCount }} />\n        <MyPopup content=\"Comment on post\">\n          <Button as={Link} to={`/posts/${id}`} labelPosition=\"right\">\n            <Button color=\"blue\" basic>\n              <Icon name=\"comments\" />\n            </Button>\n            <Label basic color=\"blue\" pointing=\"left\">\n              {commentCount}\n            </Label>\n          </Button>\n        </MyPopup>\n        {user && user.username === username && (\n          <EditModal postId={id} body={body} username={username} />\n        )}\n        {user && user.username === username && <DeleteButton postId={id} />}\n      </Card.Content>\n    </Card>\n  );\n}\n\nexport default PostCard;\n","/Users/wenjen/Desktop/node-apps/react-social-media/client/src/util/hooks.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/context/auth.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/util/AuthRoute.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/PostForm.js",["85"],"import React, { useState } from 'react';\nimport { Button, Form } from 'semantic-ui-react';\nimport { gql, useMutation } from '@apollo/client';\n\nimport { useForm } from '../util/hooks';\nimport { FETCH_POSTS_QUERY } from '../util/graphql';\n\nfunction PostForm() {\n  const [errors, setErrors] = useState({});\n\n  const { values, onChange, onSubmit } = useForm(createPostCallback, {\n    body: '',\n  });\n\n  const [createPost, { error }] = useMutation(CREATE_POST_MUTATION, {\n    variables: values,\n    update(proxy, result) {\n      const data = proxy.readQuery({\n        query: FETCH_POSTS_QUERY,\n      });\n      let newData = [result.data.createPost, ...data.getPosts];\n      proxy.writeQuery({\n        query: FETCH_POSTS_QUERY,\n        data: {\n          ...data,\n          getPosts: {\n            newData,\n          },\n        },\n      });\n      values.body = '';\n    },\n    onError(err) {\n      setErrors(err.graphQLErrors[0].extensions.exception.errors);\n    },\n  });\n\n  function createPostCallback() {\n    createPost();\n  }\n\n  return (\n    <>\n      <Form onSubmit={onSubmit}>\n        <h2>Create a post:</h2>\n        <Form.Field>\n          <Form.Input\n            placeholder=\"Hi World!\"\n            name=\"body\"\n            onChange={onChange}\n            value={values.body}\n            error={errors && errors.general}\n          />\n          <Button type=\"submit\" color=\"teal\">\n            Submit\n          </Button>\n        </Form.Field>\n      </Form>\n    </>\n  );\n}\n\nconst CREATE_POST_MUTATION = gql`\n  mutation createPost($body: String!) {\n    createPost(body: $body) {\n      id\n      body\n      createdAt\n      username\n      likes {\n        id\n        username\n        createdAt\n      }\n      likeCount\n      comments {\n        id\n        body\n        username\n        createdAt\n      }\n      commentCount\n    }\n  }\n`;\n\nexport default PostForm;\n","/Users/wenjen/Desktop/node-apps/react-social-media/client/src/util/graphql.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/LikeButton.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/pages/SinglePost.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/DeleteButton.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/util/MyPopup.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/NewPostNotification.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/EditModal.js",["86","87","88","89","90","91"],"import React, { useState, useContext } from 'react';\nimport {\n  Button,\n  Header,\n  Popup,\n  Icon,\n  Image,\n  Modal,\n  Form,\n} from 'semantic-ui-react';\nimport { gql, useMutation } from '@apollo/client';\nimport { AuthContext } from '../context/auth';\nimport { useForm } from '../util/hooks';\n\nfunction EditModal({ username, postId, body }) {\n  const [open, setOpen] = useState(false);\n  const { user } = useContext(AuthContext);\n  const [isEditing, setIsEditing] = useState(false);\n  const [errors, setErrors] = useState({});\n  const { values, onChange, onSubmit } = useForm(editPostCallback, {\n    body: body,\n  });\n\n  const [editPost, { error }] = useMutation(EDIT_POST_MUTATION, {\n    update(proxy, result) {\n      console.log(result);\n      console.log('hi');\n    },\n    variables: {\n      postId,\n      body: values.body,\n    },\n    onError(error) {\n      console.log(error);\n    },\n  });\n\n  function editPostCallback() {\n    setOpen(false);\n    editPost();\n  }\n\n  return (\n    <Modal\n      onClose={() => setOpen(false)}\n      onOpen={() => setOpen(true)}\n      open={open}\n      trigger={\n        <Button as=\"div\" color=\"instagram\" floated=\"right\">\n          <Icon name=\"edit outline\" style={{ margin: 0 }} />\n        </Button>\n      }\n    >\n      <Modal.Header>{username}</Modal.Header>\n      <Modal.Content image>\n        <Image\n          size=\"small\"\n          src=\"https://react.semantic-ui.com/images/avatar/large/molly.png\"\n          wrapped\n        />\n        <Modal.Description style={{ width: '100%' }}>\n          <Header>Edit Post</Header>\n          <Form onSubmit={onSubmit}>\n            <Form.Field>\n              <Form.Input\n                name=\"body\"\n                onChange={onChange}\n                value={values.body}\n                error={errors && errors.general}\n              />\n            </Form.Field>\n          </Form>\n        </Modal.Description>\n      </Modal.Content>\n      <Modal.Actions>\n        <Button color=\"red\" onClick={() => setOpen(false)}>\n          Cancel\n        </Button>\n        <Button\n          content=\"Update Post\"\n          labelPosition=\"right\"\n          icon=\"checkmark\"\n          onClick={onSubmit}\n          positive\n        />\n      </Modal.Actions>\n    </Modal>\n  );\n}\n\nconst EDIT_POST_MUTATION = gql`\n  mutation editPost($postId: ID!, $body: String!) {\n    editPost(postId: $postId, body: $body) {\n      id\n      body\n      createdAt\n      username\n      likes {\n        id\n        username\n        createdAt\n      }\n      likeCount\n      comments {\n        id\n        body\n        username\n        createdAt\n      }\n      commentCount\n    }\n  }\n`;\n\nexport default EditModal;\n",{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","severity":1,"messa