      createdAt\n      username\n      likes {\n        id\n        username\n        createdAt\n      }\n      likeCount\n      comments {\n        id\n        body\n        username\n        createdAt\n      }\n      commentCount\n    }\n  }\n`;\n\nexport default PostForm;\n","/Users/wenjen/Desktop/node-apps/react-social-media/client/src/util/graphql.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/LikeButton.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/pages/SinglePost.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/DeleteButton.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/util/MyPopup.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/NewPostNotification.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":15,"column":24,"nodeType":"85","messageId":"86","endLine":15,"endColumn":29},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"no-unused-vars","'error' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]