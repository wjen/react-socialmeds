        error={errors && errors.general}\n          />\n          <Button type=\"submit\" color=\"teal\">\n            Submit\n          </Button>\n        </Form.Field>\n      </Form>\n    </>\n  );\n}\n\nconst CREATE_POST_MUTATION = gql`\n  mutation createPost($body: String!) {\n    createPost(body: $body) {\n      id\n      body\n      createdAt\n      username\n      likes {\n        id\n        username\n        createdAt\n      }\n      likeCount\n      comments {\n        id\n        body\n        username\n        createdAt\n      }\n      commentCount\n    }\n  }\n`;\n\nexport default PostForm;\n","/Users/wenjen/Desktop/node-apps/react-social-media/client/src/util/graphql.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/LikeButton.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/pages/SinglePost.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/DeleteButton.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/util/MyPopup.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/NewPostNotification.js",[],"/Users/wenjen/Desktop/node-apps/react-social-media/client/src/components/EditModal.js",["85","86","87","88","89","90"],"import React, { useState, useContext } from 'react';\nimport {\n  Button,\n  Header,\n  Popup,\n  Icon,\n  Image,\n  Modal,\n  Form,\n} from 'semantic-ui-react';\nimport { gql, useMutation } from '@apollo/client';\nimport { AuthContext } from '../context/auth';\nimport { useForm } from '../util/hooks';\n\nfunction EditModal({ username, postId, body }) {\n  const [open, setOpen] = useState(false);\n  const { user } = useContext(AuthContext);\n  const [isEditing, setIsEditing] = useState(false);\n  const [errors, setErrors] = useState({});\n  const { values, onChange, onSubmit } = useForm(editPostCallback, {\n    body: body,\n  });\n\n  const [editPost, { error }] = useMutation(EDIT_POST_MUTATION, {\n    update(proxy, result) {\n      console.log(result);\n      console.log('hi');\n    },\n    variables: {\n      postId,\n      body: values.body,\n    },\n    onError(error) {\n      console.log(error);\n    },\n  });\n\n  function editPostCallback() {\n    setOpen(false);\n    editPost();\n  }\n\n  return (\n    <Modal\n      onClose={() => setOpen(false)}\n      onOpen={() => setOpen(true)}\n      open={open}\n      trigger={\n        <Button as=\"div\" color=\"instagram\" floated=\"right\">\n          <Icon name=\"edit outline\" style={{ margin: 0 }} />\n        </Button>\n      }\n    >\n      <Modal.Header>{username}</Modal.Header>\n      <Modal.Content image>\n        <Image\n          size=\"small\"\n          src=\"https://react.semantic-ui.com/images/avatar/large/molly.png\"\n          wrapped\n        />\n        <Modal.Description style={{ width: '100%' }}>\n          <Header>Edit Post</Header>\n          <Form onSubmit={onSubmit}>\n            <Form.Field>\n              <Form.Input\n                name=\"body\"\n                onChange={onChange}\n                value={values.body}\n                error={errors && errors.general}\n              />\n            </Form.Field>\n          </Form>\n        </Modal.Description>\n      </Modal.Content>\n      <Modal.Actions>\n        <Button color=\"red\" onClick={() => setOpen(false)}>\n          Cancel\n        </Button>\n        <Button\n          content=\"Update Post\"\n          labelPosition=\"right\"\n          icon=\"checkmark\"\n          onClick={onSubmit}\n          positive\n        />\n      </Modal.Actions>\n    </Modal>\n  );\n}\n\nconst EDIT_POST_MUTATION = gql`\n  mutation editPost($postId: ID!, $body: String!) {\n    editPost(postId: $postId, body: $body) {\n      id\n      body\n      createdAt\n      username\n      likes {\n        id\n        username\n        createdAt\n      }\n      likeCount\n      comments {\n        id\n        body\n        username\n        createdAt\n      }\n      commentCount\n    }\n  }\n`;\n\nexport default EditModal;\n",{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":2,"column":44,"nodeType":"97","messageId":"98","endLine":2,"endColumn":49},{"ruleId":"95","severity":1,"message":"99","line":15,"column":24,"nodeType":"97","messageId":"98","endLine":15,"endColumn":29},{"ruleId":"95","severity":1,"message":"96","line":5,"column":3,"nodeType":"97","messageId":"98","endLine":5,"endColumn":8},{"ruleId":"95","severity":1,"message":"100","line":17,"column":11,"nodeType":"97","messageId":"98","endLine":17,"endColumn":15},{"ruleId":"95","severity":1,"message":"101","line":18,"column":10,"nodeType":"97","messageId":"98","endLine":18,"endColumn":19},{"ruleId":"95","severity":1,"message":"102","line":18,"column":21,"nodeType":"97","messageId":"98","endLine":18,"endColumn":33},{"ruleId":"95","severity":1,"message":"103","line":19,"column":18,"nodeType":"97","messageId":"98","endLine":19,"endColumn":27},{"ruleId":"95","severity":1,"message":"99","line":24,"column":22,"nodeType":"97","messageId":"98","endLine":24,"endColumn":27},"no-native-reassign",["104"],"no-negated-in-lhs",["105"],"no-unused-vars","'Popup' is defined but never used.","Identifier","unusedVar","'error' is assigned a value but never used.","'user' is assigned a value but never used.","'isEditing' is assigned a value but never used.","'setIsEditing' is assigned a value but never used.","'setErrors' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]